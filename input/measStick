import firstOrderLogic

typeSystem measStick =
    imports firstOrderLogic
    types extend, chop, longer, shorter, equals, _:seg, unitSeg
    order unitSeg < seg,
          seg < obj,
          longer < binRel,
          shorter < binRel,
          extend < terRel,
          chop < terRel,
          equals < binRel

conSpec measStickG:measStick =
  imports firstOrderLogicG
  constructors
  dummy: [universal] -> universal


import firstOrderLogic

typeSystem solarSystem =
    imports firstOrderLogic
    types revolves, mass, sun, force, gravity, centrifugal, dist, planet, _:time, zero
    order planet < obj,
          sun < obj,
          revolves < obj,
          time < obj,
          mass < unRel,
          shorter < binRel,
          gravity < terRel,
          centrifugal < terRel,
          more < binRel,
          zero < object

conSpec solarSystemG:solarSystem =
  imports firstOrderLogicG
  constructors
  dummy: [universal] -> universal

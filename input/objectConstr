import firstOrderLogic

typeSystem objectConstr =
    imports firstOrderLogic
    types _:obj, _:formula, half, matches, combine, split, wholeOb,
            binRel, binOp, terOp
    order larger < binRel,
          smaller < binRel,
          combine < terOp,
          split < terOp
          half < binOp

conSpec objectConstrG:objectConstr =
  imports firstOrderLogicG
  constructors
    prefixBinRel : [binRel, obj, obj] -> formula,
    prefixTerOp : [terOp, obj, obj, obj] -> obj